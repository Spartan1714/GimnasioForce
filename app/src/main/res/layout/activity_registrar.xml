<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black">

    <TextView
        android:id="@+id/txtLoginn"
        android:layout_width="101dp"
        android:layout_height="38dp"
        android:layout_marginBottom="4dp"
        android:text="Login"
        android:textColor="@android:color/holo_green_dark"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.812"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnRegistrar"
        app:layout_constraintVertical_bias="0.6" />

    <Button
        android:id="@+id/btnRegistrar"
        android:layout_width="157dp"
        android:layout_height="42dp"
        android:layout_marginBottom="72dp"
        android:backgroundTint="@android:color/holo_green_light"
        android:text="REGISTRARSE"
        app:cornerRadius="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"

        />

    <EditText
        android:id="@+id/txtPass"
        android:layout_width="348dp"
        android:layout_height="43dp"
        android:layout_marginBottom="116dp"
        android:ems="10"
        android:inputType="textPassword"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.488"
        app:layout_constraintStart_toStartOf="parent"

        />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="98dp"
        android:layout_height="33dp"
        android:layout_marginBottom="176dp"
        android:text="ContaseÃ±a:"
        android:textColor="@android:color/holo_orange_dark"
        android:textColorHint="@color/white"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.152"
        app:layout_constraintStart_toStartOf="parent"

        />

    <EditText
        android:id="@+id/txtEmail"
        android:layout_width="314dp"
        android:layout_height="44dp"
        android:ems="10"
        android:hint="ejemplo@gmail.com"
        android:textColorHint="@color/white"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.372"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView18"
        app:layout_constraintVertical_bias="0.113" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="98dp"
        android:layout_height="33dp"
        android:layout_marginBottom="312dp"
        android:text="Correo:"
        android:textColor="@android:color/holo_orange_dark"
        android:textColorHint="@color/white"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.131"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/txtTelefono"
        android:layout_width="298dp"
        android:layout_height="36dp"
        android:backgroundTint="@color/black"
        android:ems="10"
        android:hint="000-000"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.328"
        android:textColorHint="@color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView17"
        app:layout_constraintVertical_bias="0.036" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="98dp"
        android:layout_height="33dp"
        android:layout_marginTop="416dp"
        android:text="Celular:"
        android:textColor="@android:color/holo_orange_dark"
        android:textSize="20sp"
        android:hint="color"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.131"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.961" />

    <Spinner
        android:id="@+id/pais"
        android:layout_width="303dp"
        android:layout_height="36dp"
        android:layout_marginBottom="392dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.341"
        app:layout_constraintStart_toStartOf="parent"
        android:textColorHint="@color/white"

        app:layout_constraintTop_toBottomOf="@+id/textView13"
        app:layout_constraintVertical_bias="0.203" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="98dp"
        android:layout_height="33dp"
        android:layout_marginBottom="536dp"
        android:text="Pais:"

        android:textColor="@android:color/holo_orange_dark"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.131"
        app:layout_constraintStart_toStartOf="parent"
        android:textColorHint="@color/white"
        app:layout_constraintVertical_bias="0.027" />

    <EditText
        android:id="@+id/txtApellido"
        android:layout_width="291dp"
        android:layout_height="35dp"
        android:layout_marginBottom="520dp"
        android:ems="10"
        android:hint="Ingrese su apellido"
        android:inputType="textPersonName"
        android:textColorHint="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.29"
        app:layout_constraintStart_toStartOf="parent"

        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:layout_constraintVertical_bias="0.168" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="98dp"
        android:layout_height="33dp"
        android:layout_marginBottom="644dp"
        android:text="Apellido:"
        android:textColor="@android:color/holo_orange_dark"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.116"
        app:layout_constraintStart_toStartOf="parent"

        />

    <EditText
        android:id="@+id/txtNombre"
        android:layout_width="293dp"
        android:layout_height="42dp"
        android:layout_marginBottom="636dp"
        android:ems="10"
        android:hint="Ingrese su nombre"
        android:inputType="textPersonName"
        android:textColorHint="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.28"
        app:layout_constraintStart_toStartOf="parent"

        app:layout_constraintTop_toBottomOf="@+id/textView4"
        app:layout_constraintVertical_bias="0.185" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="98dp"
        android:layout_height="33dp"
        android:layout_marginBottom="748dp"
        android:text="Nombre:"
        android:textColor="@android:color/holo_orange_dark"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.116"
        app:layout_constraintStart_toStartOf="parent"

        />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="182dp"
        android:layout_height="50dp"
        android:text="REGISTRARSE"
        android:textColor="@android:color/holo_orange_dark"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.464"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.019" />
</androidx.constraintlayout.widget.ConstraintLayout>